{
	"useTabs": false,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"tabWidth": 4,
	"bracketSameLine": false,
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": ["prettier-plugin-svelte",
	"prettier-plugin-tailwindcss"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
